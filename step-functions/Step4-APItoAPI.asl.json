{
  "Comment": "This is your state machine - fetch from HTTP and write to CSV",
  "StartAt": "Lambda Invoke - Fetch from HTTP",
  "States": {
    "Lambda Invoke - Fetch from HTTP": {
      "Type": "Task",
      "Resource": "arn:aws:states:::lambda:invoke",
      "OutputPath": "$.Payload",
      "Parameters": {
        "Payload.$": "$",
        "FunctionName": "arn:aws:lambda:us-east-2:722716315591:function:myNodeJSHTTPFunction:$LATEST"
      },
      "Retry": [
        {
          "ErrorEquals": [
            "Lambda.ServiceException",
            "Lambda.AWSLambdaException",
            "Lambda.SdkClientException"
          ],
          "IntervalSeconds": 2,
          "MaxAttempts": 6,
          "BackoffRate": 2
        }
      ],
      "Next": "API Gateway Request"
    },
    "API Gateway Request": {
      "Type": "Task",
      "Resource": "arn:aws:states:::apigateway:invoke",
      "Parameters": {
        "ApiEndpoint": "m7l71m8adc.execute-api.us-east-2.amazonaws.com",
        "Method": "POST",
        "Headers": {
          "Header1": [
            "HeaderValue1"
          ],
          "Header2": [
            "HeaderValue2",
            "HeaderValue3"
          ]
        },
        "Stage": "test",
        "Path": "/resource1",
        "QueryParameters": {
          "QueryParameter1": [
            "QueryParameterValue1"
          ],
          "QueryParameter2": [
            "QueryParameterValue2",
            "QueryParameterValue3"
          ]
        },
        "RequestBody": {
          "Payload": "Hello from Step Functions!"
        },
        "AuthType": "IAM_ROLE"
      },
      "End": true
    }
  }
}